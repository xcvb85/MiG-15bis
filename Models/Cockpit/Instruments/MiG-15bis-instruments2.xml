<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
  <path>MiG-15bis-instruments2.gltf</path>
  <animation>
    <type>rotate</type>
    <object-name>Alt_NX10</object-name>
    <property>fdm/jsbsim/systems/instr/VD-17/gauge/altitude-m</property>
    <factor>0.36</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Alt_NX100</object-name>
    <property>fdm/jsbsim/systems/instr/VD-17/gauge/altitude-m</property>
    <factor>0.036</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Alt_Corplate</object-name>
    <property>fdm/jsbsim/systems/instr/VD-17/pot/pressure-mmHg</property>
    <factor>3.0</factor>
    <offset-deg>-2280.0</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>knob</type>
    <object-name>Alt_Vern</object-name>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/instr/VD-17/pot/pressure-mmHg</property>
        <factor>1</factor>
        <min>700.0</min>
        <max>820.0</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>IAS_Needle</object-name>
    <property>fdm/jsbsim/velocities/vc-kts</property>
    <factor>-0.66672</factor>
    <offset-deg>-0.65</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>TAS_Needle</object-name>
    <property>fdm/jsbsim/velocities/vtrue-kts</property>
    <factor>-0.66672</factor>
    <offset-deg>35.5</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>GC_MDisk</object-name>
    <object-name>GC_Needle</object-name>
    <object-name>GC_CDisk</object-name>
    <property>fdm/jsbsim/systems/instr/DGMK-3/pot/heading-bug-deg</property>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>GC_MDisk</object-name>
    <object-name>GC_Needle</object-name>
    <property>fdm/jsbsim/systems/instr/DGMK-3/gauge/heading-deg</property>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>knob</type>
    <object-name>GC_Vern</object-name>
    <property>fdm/jsbsim/systems/instr/DGMK-3/pot/heading-bug-deg</property>
    <factor>10</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.05334</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>fdm/jsbsim/systems/instr/DGMK-3/pot/heading-bug-deg</property>
        <factor>1</factor>
        <min>0</min>
        <max>360</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Mach_Needle</object-name>
    <property>velocities/mach</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>60.0</dep></entry>
      <entry><ind>0.3</ind><dep>90.0</dep></entry>
      <entry><ind>0.55</ind><dep>180.0</dep></entry>
      <entry><ind>0.8</ind><dep>270.0</dep></entry>
      <entry><ind>0.95</ind><dep>324.0</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Mot_Needle</object-name>
    <property>fdm/jsbsim/systems/instr/EMI-3R/gauge/fuel-pressure-kgf_cm2</property>
    <factor>2.7</factor>
    <offset-deg>45.0</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.008</z-m>

    <object-name>RA_VDiapazone</object-name>
    <property>fdm/jsbsim/systems/instr/UV-2/switch/range</property>
    <factor>90.0</factor>
    <offset-deg>-45.0</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0.038</y-m>
      <z-m>0.038</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>RA_VOnoff</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-toggle</command>
        <property>fdm/jsbsim/systems/instr/UV-2/switch/power</property>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-toggle</command>
        <property>fdm/jsbsim/systems/instr/UV-2/switch/power</property>
      </binding>
    </action>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>RA_VDiapazone</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-toggle</command>
        <property>fdm/jsbsim/systems/instr/UV-2/switch/range</property>
      </binding>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-toggle</command>
        <property>fdm/jsbsim/systems/instr/UV-2/switch/range</property>

    <object-name>RA_Needle</object-name>
    <property>fdm/jsbsim/systems/instr/UV-2/gauge/height-m</property>
    <factor>2.0</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>

      <entry><ind>98.42</ind><dep>90.0</dep></entry>
      <entry><ind>49.21</ind><dep>45.0</dep></entry>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>-49.21</ind><dep>-45.0</dep></entry>
      <entry><ind>-98.42</ind><dep>-90.0</dep></entry>
      <entry><ind>-246.06</ind><dep>-180.0</dep></entry>
      <entry><ind>-10000.0</ind><dep>-180.0</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>  
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>

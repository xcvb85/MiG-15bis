  </animation>

  <animation>
    <type>rotate</type>
    <object-name>TC_Needle</object-name>
    <property>fdm/jsbsim/systems/instr/TE-15/gauge/rpm</property>
    <interpolation>
      <entry><ind>0.0</ind><dep>0.0</dep></entry>
      <entry><ind>15000.0</ind><dep>540.0</dep></entry>
    </interpolation>

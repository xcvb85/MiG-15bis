      <y-m>-0.018713</y-m>
      <z-m>-0.012073</z-m>
    </center>  
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Mot_RNeedle</object-name>
    <property>fdm/jsbsim/systems/instr/EMI-3R/gauge/oil-temperature-degC</property>
    <factor>0.715</factor>
    <offset-deg>35.75</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0.01791</y-m>
      <z-m>-0.012073</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>MP_Needle</object-name>
    <property>fdm/jsbsim/systems/instr/EM-10/gauge/fuel-pressure-kgf_cm2</property>
    <factor>2.5</factor>
    <offset-deg>22.0</offset-deg>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.0</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>OP_Needle</object-name>
    <property>consumables/oxygen/pressure-norm</property>
    <interpolation> 
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>1</ind><dep>180</dep></entry>
    </interpolation> 
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>  
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>

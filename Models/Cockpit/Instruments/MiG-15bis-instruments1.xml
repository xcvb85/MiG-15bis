    <property>fdm/jsbsim/systems/instr/TGZ-47/gauge/EGT-degC</property>
    <interpolation>
      <entry><ind>0</ind><dep>6</dep></entry>
      <entry><ind>400</ind><dep>20</dep></entry>
      <entry><ind>500</ind><dep>36</dep></entry>
      <entry><ind>600</ind><dep>53</dep></entry>
      <entry><ind>700</ind><dep>72</dep></entry>
      <entry><ind>800</ind><dep>90</dep></entry>
      <entry><ind>900</ind><dep>107</dep></entry>
      <entry><ind>1100</ind><dep>115</dep></entry>
      <entry><ind>1200</ind><dep>115</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>-0.014</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>HO_Ball</object-name>
    <property>fdm/jsbsim/systems/instr/AGI-1/gauge/ball-deg</property>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.133</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>HO_Sphere</object-name>
    <property>fdm/jsbsim/systems/instr/AGI-1/gauge/roll-deg</property>
    <center>
      <x-m>-0.054</x-m>
      <y-m>0</y-m>
      <z-m>0.009</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>HO_Sphere</object-name>
    <property>fdm/jsbsim/systems/instr/AGI-1/gauge/pitch-deg</property>
    <center>
      <x-m>-0.054</x-m>
      <y-m>0</y-m>
      <z-m>0.009</z-m>
    </center>
    <axis>
      <x>0</x>
